prefix=@prefix@
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: Log4g
Description: A GObject-based logging utility
URL: http://msteinert.github.com/log4g/
Version: @VERSION@
Requires.private: @log4g_glib_version@ @log4g_gmodule_version@ @log4g_gobject_version@ @log4g_gthread_version@ @log4g_libxml_version@ 
Libs: -L${libdir} -llog4g
CFlags: -I${includedir}
